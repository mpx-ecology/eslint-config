{
  "name": "@mpxjs/eslint-config",
  "type": "module",
  "version": "2.0.5",
  "description": "Mpx's ESLint config presets",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nr gen && tsup --clean --dts",
    "gen": "tsx scripts/typegen.ts",
    "dev": "npx @eslint/config-inspector --config eslint.config.ts",
    "stub": "tsup",
    "watch": "tsup --watch",
    "lint": "eslint",
    "test": "vitest",
    "prepack": "nr build",
    "release": "bumpp && pnpm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mpx-ecology/eslint-config.git"
  },
  "keywords": [
    "eslint-config"
  ],
  "author": "pagnkelly@github.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mpx-ecology/eslint-config/issues"
  },
  "homepage": "https://github.com/mpx-ecology/eslint-config#readme",
  "peerDependencies": {
    "eslint": "^9.10.0"
  },
  "devDependencies": {
    "@antfu/ni": "^24.3.0",
    "@eslint/config-inspector": "^1.0.2",
    "@mpxjs/core": "^2.10.4",
    "@mpxjs/eslint-config": "workspace:*",
    "@types/node": "^22.14.1",
    "bumpp": "^8.2.1",
    "eslint": "^9.24.0",
    "eslint-typegen": "^2.1.0",
    "execa": "^9.5.2",
    "tinyglobby": "^0.2.13",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1",
    "vue": "^3.5.13"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^8.27.0",
    "@typescript-eslint/parser": "^5.33.1",
    "eslint-flat-config-utils": "^2.0.1",
    "eslint-plugin-mpx": "0.2.22",
    "eslint-plugin-n": "^17.17.0",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.0.0",
    "jiti": "^2.4.2",
    "local-pkg": "^1.1.1",
    "mpx-eslint-parser": "^0.0.14",
    "vue-eslint-parser": "^10.1.3"
  }
}
