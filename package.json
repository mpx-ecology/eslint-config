{
  "name": "@mpxjs/eslint-config",
  "type": "module",
  "version": "1.0.10",
  "private": true,
  "description": "Mpx's ESLint config presets",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "build": "nr gen && tsup --clean --dts",
    "gen": "tsx scripts/typegen.ts",
    "dev": "npx @eslint/config-inspector --config eslint.config.ts",
    "stub": "tsup",
    "watch": "tsup --watch",
    "lint": "eslint",
    "test": "vitest",
    "release": "bumpp && pnpm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mpx-ecology/eslint-config.git"
  },
  "keywords": [
    "eslint-config"
  ],
  "author": "pagnkelly@github.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mpx-ecology/eslint-config/issues"
  },
  "homepage": "https://github.com/mpx-ecology/eslint-config#readme",
  "devDependencies": {
    "@antfu/ni": "^24.3.0",
    "@eslint/config-inspector": "^1.0.2",
    "@mpxjs/core": "^2.10.4",
    "@mpxjs/eslint-config": "workspace:*",
    "@types/node": "^22.14.1",
    "bumpp": "^8.2.1",
    "eslint": "^9.24.0",
    "eslint-typegen": "^2.1.0",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1"
  },
  "dependencies": {
    "eslint-flat-config-utils": "^2.0.1",
    "eslint-plugin-mpx": "0.2.20-3",
    "local-pkg": "^1.1.1"
  }
}
