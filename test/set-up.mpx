<script setup>
// 为了不把这些import的内容导出去 所有模版需要的变量将都在defineExpose方法导出，mpx/script-setup-uses-vars规则则无效了
import { reactive, onLoad } from '@mpxjs/core'
const state = reactive({ count: 0 })

onLoad(() => {
  console.log('onLoad lifetime trigger')
})

let props;
let a = defineProps({
  a: 123
})

const baseStyle = {
  color: 'red'
}

function increment() {
  state.count++
}
</script>
<template>
  <view bindtap="increment">{{baseStyle}}</view>
</template>